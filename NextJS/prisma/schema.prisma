generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tabla1 {
  column1 String? @db.VarChar
  id      Int     @id(map: "test_table_pk")
  tabla2  tabla2  @relation(fields: [id], references: [id], onDelete: Cascade, map: "tabla1_tabla2_fk")
}

model tabla2 {
  id     Int     @id(map: "tabla2_pk")
  cosas  String? @db.VarChar
  tabla1 tabla1?
}
